{
  "name": "@openfn/language-qontak",
  "version": "1.0.0",
  "description": "A Qontak APIs language package for use with Open Function",
  "homepage": "https://docs.openfn.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/openfn/adaptors.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "types": "types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist types docs",
    "build": "pnpm clean && build-adaptor qontak",
    "pack": "pnpm pack --pack-destination ../../dist",
    "test": "mocha --experimental-specifier-resolution=node --no-warnings",
    "test:watch": "mocha -w"
  },
  "author": "Open Function Group",
  "license": "LGPLv3",
  "files": [
    "dist/",
    "types/",
    "ast.json",
    "configuration-schema.json"
  ],
  "dependencies": {
    "@openfn/language-common": "workspace:1.7.5",
    "cheerio": "^1.0.0-rc.10",
    "cheerio-tableparser": "^1.0.1",
    "csv-parse": "^4.10.1",
    "fast-safe-stringify": "^2.0.7",
    "form-data": "^3.0.0",
    "limiter": "^2.1.0",
    "lodash": "^4.17.19",
    "request": "^2.88.2",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@openfn/buildtools": "workspace:^1.0.2",
    "@openfn/simple-ast": "0.4.1",
    "chai": "4.3.6",
    "deep-eql": "4.1.1",
    "esno": "^0.16.3",
    "mocha": "9.2.2",
    "nock": "13.2.9",
    "rimraf": "^3.0.2"
  },
  "main": "dist/index.cjs"
}
