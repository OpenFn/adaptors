{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "server": {
            "title": "Data Center",
            "type": "string",
            "description": "Mailchimp Data Center for your account",
            "minLength": 1,
            "examples": [
                "us11",
                "uk8",
                "in10"
            ]
        },
        "apiKey": {
            "title": "API Key",
            "type": "string",
            "description": "Mailchimp API Key",
            "minLength": 1,
            "examples": [
                "0eb22c7b4a1c5bcd789379bf8a92902d-us13"
            ]
        }
    },
    "type": "object",
    "additionalProperties": true,
    "required": [
        "server",
        "apiKey"
    ]
}