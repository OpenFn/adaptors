{
  "name": "@openfn/language-http",
  "version": "6.4.1",
  "description": "An HTTP request language package for use with Open Function",
  "homepage": "https://docs.openfn.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/openfn/adaptors.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "types": "types/index.d.ts",
  "scripts": {
    "clean": "rimraf dist types docs",
    "build": "pnpm clean && build-adaptor http",
    "pack": "pnpm pack --pack-destination ../../dist",
    "test": "mocha --require mocha-suppress-logs --experimental-specifier-resolution=node --no-warnings",
    "test:watch": "mocha -w",
    "lint": "eslint src"
  },
  "author": "Open Function Group",
  "license": "LGPLv3",
  "files": [
    "dist/",
    "types/",
    "ast.json",
    "configuration-schema.json"
  ],
  "dependencies": {
    "@openfn/language-common": "workspace:*",
    "cheerio": "1.0.0-rc.12",
    "cheerio-tableparser": "^1.0.1"
  },
  "devDependencies": {
    "@openfn/simple-ast": "0.4.1",
    "chai": "^4.3.10",
    "deep-eql": "4.1.1",
    "esno": "^0.16.3",
    "mocha": "9.2.2",
    "mocha-suppress-logs": "^0.4.1",
    "rimraf": "^3.0.2"
  },
  "main": "dist/index.cjs"
}
