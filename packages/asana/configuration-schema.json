{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "apiVersion": {
      "title": "API Version",
      "type": "string",
      "description": "The API version",
      "examples": [
        "1.0"
      ],
      "default": "1.0"
    },
    "token": {
      "title": "Token",
      "type": "string",
      "description": "The API token",
      "examples": [
        "sample-tokenyWSJdXBACMLLWMNGgADFA"
      ]
    }
  },
  "type": "object",
  "additionalProperties": true,
  "required": [
    "token"
  ]
}