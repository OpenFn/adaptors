{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "domain": {
            "title": "Domain Name",
            "type": "string",
            "description": "Mailgun API Domain Name",
            "format": "string",
            "minLength": 1,
            "examples": [
                "sandbox-123.mailgun.org"
            ]
        },
        "apiKey": {
            "title": "API Key",
            "type": "string",
            "description": "Mailgun API Key",
            "writeOnly": true,
            "minLength": 1,
            "examples": [
                "otherThiNGfSECret"
            ]
        }
    },
    "type": "object",
    "additionalProperties": true,
    "required": [
        "domain",
        "apiKey"
    ]
}